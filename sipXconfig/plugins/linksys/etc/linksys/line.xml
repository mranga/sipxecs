      <profileName>Share_Line_Appearance</profileName>
      <type>
        <enum>
          <option>
            <value>private</value>
          </option>
          <option>
            <value>shared</value>
          </option>
        </enum>
      </type>
      <value>private</value>
    </setting>
    <setting name="Shared_User_ID" advanced="yes">
      <label>Shared User ID</label>
      <profileName>Share_Line_Appearance</profileName>
      <value />
