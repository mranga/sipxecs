  <group name="NAT_Settings">
    <label>NAT Settings</label>
    <setting name="NAT_Mapping_Enable">
      <label>NAT Mapping Enable</label>
      <profileName>NAT_Settings</profileName>
      <type refid="yes-no" />
      <value>no</value>
    </setting>
    <setting name="NAT_Keep_Alive_Enable" advanced="yes">
