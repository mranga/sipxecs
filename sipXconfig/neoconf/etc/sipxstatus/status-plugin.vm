<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<subscribe-server-plugins>
  <subscribe-plugin>
    <event-type>message-summary</event-type>
    <permissionMatch>
      <permission>Voicemail</permission>
    </permissionMatch>
    <load-library></load-library>
    <plugin-factory>MwiFactory</plugin-factory>
    <plugin-data>
      <voicemail-cgi-url>https://localhost:${mediaService.voicemailHttpsPort}/cgi-bin/voicemail/mediaserver.cgi</voicemail-cgi-url>
    </plugin-data>
  </subscribe-plugin>
</subscribe-server-plugins>
